{
  "manifest_version": 3,
  "name": "Random Joke Generator by Ros Sheketa",
  "version": "0.0.0.1",
  "description": "Get a random joke to brighten your day!",
  "action": {
    "default_popup": "index.html",
    "default_title": "Random Joke"
  },
  "icons": {
    "48": "r-logo.png",
    "96": "r-logo.png",
    "128": "r-logo.png"
  },
  "host_permissions": [
    "https://v2.jokeapi.dev/*"
  ]
}
